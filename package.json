		"ts-loader": "^9.2.6",
		"webpack": "^5.66.0",
		"webpack-cli": "^4.9.1",
		"@types/webpack-env": "^1.16.3",
		"assert": "^2.0.0",
		"process": "^0.11.10"
	}
