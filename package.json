    "keybindings": [
      {
        "key": "ctrl+x ctrl+t",
        "command": "groog.toggleQMK"
      },
      {
        "key": "ctrl+j",
        "command": "groog.toggleMarkMode"
      },
      {
        "key": "ctrl+g",
        "command": "groog.ctrlG",
        "when": "!sideBarFocus && !inQuickOpen && !suggestWidgetVisible"
      },
      {
        "key": "ctrl+g",
        "command": "workbench.action.focusActiveEditorGroup",
        "when": "sideBarFocus && !inQuickOpen && !suggestWidgetVisible"
      },
      {
        "key": "ctrl+g",
        "command": "workbench.action.closeQuickOpen",
        "when": "inQuickOpen && !suggestWidgetVisible"
      },
      {
        "key": "ctrl+g",
        "command": "hideSuggestWidget",
        "when": "suggestWidgetVisible"
      },
      {
        "key": "ctrl+y",
        "command": "groog.paste"
      },
      {
        "key": "ctrl+w",
        "command": "groog.yank"
      },
      {
        "key": "ctrl+k",
        "command": "groog.kill"
      },
      {
        "key": "ctrl+x ctrl+r",
        "command": "workbench.action.reloadWindow"
      },
      {
        "key": "ctrl+v",
        "command": "groog.fall"
      },
      {
        "key": "pagedown",
        "command": "groog.fall"
      },
      {
        "key": "pageup",
        "command": "groog.jump"
      },
      {
        "key": "ctrl+l",
        "command": "groog.jump"
      },
      {
        "key": "ctrl+p",
        "command": "groog.cursorUp",
        "when": "editorTextFocus && !suggestWidgetVisible"
      },
      {
        "key": "ctrl+p",
        "command": "selectPrevSuggestion",
        "when": "editorTextFocus && suggestWidgetVisible"
      },
      {
        "key": "ctrl+p",
        "command": "workbench.action.quickOpenNavigatePreviousInFilePicker",
        "when": "inQuickOpen"
      },
      {
        "key": "up",
        "command": "groog.cursorUp",
        "when": "editorTextFocus && !suggestWidgetVisible"
      },
      {
        "key": "up",
        "command": "selectPrevSuggestion",
        "when": "editorTextFocus && suggestWidgetVisible"
      },
      {
        "key": "up",
        "command": "workbench.action.quickOpenNavigatePreviousInFilePicker",
        "when": "inQuickOpen"
      },
      {
        "key": "ctrl+n",
        "command": "groog.cursorDown",
        "when": "editorTextFocus && !suggestWidgetVisible"
      },
      {
        "key": "ctrl+n",
        "command": "selectNextSuggestion",
        "when": "editorTextFocus && suggestWidgetVisible"
      },
      {
        "key": "ctrl+n",
        "command": "workbench.action.quickOpenNavigateNextInFilePicker",
        "when": "inQuickOpen"
      },
      {
        "key": "ctrl+n",
        "command": "-workbench.action.files.newUntitledFile"
      },
      {
        "key": "down",
        "command": "groog.cursorDown",
        "when": "editorTextFocus && !suggestWidgetVisible"
      },
      {
        "key": "down",
        "command": "selectNextSuggestion",
        "when": "editorTextFocus && suggestWidgetVisible"
      },
      {
        "key": "down",
        "command": "workbench.action.quickOpenNavigateNextInFilePicker",
        "when": "inQuickOpen"
      },
      {
        "key": "left",
        "command": "groog.cursorLeft"
      },
      {
        "key": "ctrl+b",
        "command": "groog.cursorLeft"
      },
      {
        "key": "right",
        "command": "groog.cursorRight"
      },
      {
        "key": "ctrl+f",
        "command": "groog.cursorRight",
        "when": "!groog.qmk"
      },
      {
        "key": "ctrl+s",
        "command": "editor.action.nextMatchFindAction",
        "when": "!groog.qmk && findWidgetVisible"
      },
      {
        "key": "ctrl+s",
        "command": "actions.find",
        "when": "!groog.qmk && !findWidgetVisible"
      },
      {
        "key": "ctrl+s",
        "command": "groog.cursorRight",
        "when": "groog.qmk"
      },
      {
        "key": "ctrl+f",
        "command": "actions.find",
        "when": "groog.qmk && !findWidgetVisible"
      },
      {
        "key": "ctrl+f",
        "command": "editor.action.nextMatchFindAction",
        "when": "groog.qmk && findWidgetVisible"
      },
      {
        "key": "ctrl+r",
        "command": "editor.action.previousMatchFindAction"
      },
      {
        "key": "alt+b",
        "command": "groog.cursorWordLeft"
      },
      {
        "key": "alt+f",
        "command": "groog.cursorWordRight"
      },
      {
        "key": "ctrl+a",
        "command": "groog.cursorHome"
      },
      {
        "key": "home",
        "command": "groog.cursorHome"
      },
      {
        "key": "ctrl+e",
        "command": "groog.cursorEnd"
      },
      {
        "key": "end",
        "command": "groog.cursorEnd"
      },
      {
        "key": "ctrl+x ctrl+p",
        "command": "groog.cursorTop"
      },
      {
        "key": "ctrl+x ctrl+n",
        "command": "groog.cursorBottom"
      },
      {
        "key": "ctrl+x ctrl+s",
        "command": "workbench.action.files.save"
      },
      {
        "key": "ctrl+h",
        "command": "deleteLeft"
      },
      {
        "key": "ctrl+d",
        "command": "deleteRight"
      },
      {
        "key": "alt+h",
        "command": "deleteWordLeft"
      },
      {
        "key": "alt+backspace",
        "command": "deleteWordLeft"
      },
      {
        "key": "alt+d",
        "command": "deleteWordRight"
      },
      {
        "key": "ctrl+x ctrl+f",
        "command": "workbench.action.quickOpen"
      },
      {
        "key": "ctrl+q",
        "command": "workbench.action.closeEditorsAndGroup"
      },
      {
        "key": "ctrl+o",
        "command": "workbench.action.focusNextGroup"
      },
      {
        "key": "ctrl+u",
        "command": "workbench.action.focusPreviousGroup"
      },
      {
        "key": "ctrl+tab",
        "command": "workbench.action.focusNextGroup"
      },
      {
        "key": "ctrl+shift+tab",
        "command": "workbench.action.focusPreviousGroup"
      },
      {
        "key": "ctrl+pageup",
        "command": "workbench.action.focusNextGroup"
      },
      {
        "key": "ctrl+pagedown",
        "command": "workbench.action.focusPreviousGroup"
      },
      {
        "key": "ctrl+x ctrl+b",
        "command": "workbench.action.openGlobalKeybindingsFile"
      },
      {
        "key": "ctrl+x ctrl+l",
        "command": "workbench.action.gotoLine"
      },
      {
        "key": "ctrl+x ctrl+q",
        "command": "workbench.action.toggleSidebarVisibility"
      },
      {
        "key": "ctrl+shift+n",
        "command": "workbench.action.files.newUntitledFile"
      },
      {
        "key": "ctrl+i",
        "command": "editor.action.formatSelection"
      },
      {
        "key": "ctrl+/",
        "command": "undo"
      },
      {
        "key": "ctrl+;",
        "command": "editor.action.commentLine"
      },
      {
        "key": "ctrl+shift+a",
        "command": "editor.action.selectAll"
      },
      {
        "key": "ctrl+t",
        "command": "go.test.package"
      },
      {
        "key": "alt+r",
        "command": "toggleSearchEditorRegex"
      },
      {
        "key": "alt+c",
        "command": "toggleSearchEditorCaseSensitive"
      },
      {
        "key": "alt+w",
        "command": "toggleSearchEditorWholeWord"
      },
      {
        "key": "alt+f4",
        "command": "toggleSearchEditorWholeWord",
        "when": "groog.qmk"
      },
      {
        "key": "alt+x",
        "command": "workbench.action.showCommands"
      },
      {
        "key": "ctrl+x ctrl+v",
        "command": "groog.multiCommand.execute",
        "args": {
          "sequence": [
            "workbench.action.splitEditorDown",
            "workbench.action.focusPreviousGroup"
          ]
        }
      },
      {
        "key": "ctrl+x ctrl+h",
        "command": "groog.multiCommand.execute",
        "args": {
          "sequence": [
            "workbench.action.splitEditorRight",
            "workbench.action.focusPreviousGroup"
          ]
        }
      },
      {
        "key": "ctrl+x ctrl+x",
        "command": "groog.record.startRecording"
      },
      {
        "key": "alt+e",
        "command": "groog.record.endRecording",
        "when": "groog.recording"
      },
      {
        "key": "alt+e",
        "command": "groog.record.playRecording",
        "when": "!groog.recording"
      },
      {
        "key": "a",
        "command": "groog.a"
      },
      {
        "key": "shift+a",
        "command": "groog.A"
      },
      {
        "key": "b",
        "command": "groog.b"
      },
      {
        "key": "shift+b",
        "command": "groog.B"
      }
		]
	},
	"scripts": {
		"test": "vscode-test-web --browserType=chromium --extensionDevelopmentPath=. --extensionTestsPath=dist/web/test/suite/index.js",
		"pretest": "npm run compile-web",
		"vscode:prepublish": "npm run package-web",
		"compile-web": "webpack",
		"watch-web": "webpack --watch",
		"package-web": "webpack --mode production --devtool hidden-source-map",
		"lint": "eslint src --ext ts",
		"run-in-browser": "vscode-test-web --browserType=chromium --extensionDevelopmentPath=. ."
	},
	"devDependencies": {
		"@types/vscode": "^1.64.0",
